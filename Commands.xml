<commands>
    <!-- This will bind Alt+1, Alt+2, etc to the ability bar if they aren't already bound to something else -->
    <command ID="CmdAbility1">
        <keyboardBind Modifier="alt" Key="1"/>
    </command>
    <command ID="CmdAbility2">
